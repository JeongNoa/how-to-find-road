start = (0, 0)
width = int(input())
height = int(input())
def factorial(a):
    result = 1
    for i in range(1, a+1):
        result = result * i
    return result
result = int(factorial(width+height)/factorial(width)/factorial(height))
print("(0, 0)에서 (%d, %d)로 이동하는 최단거리의 수는 %d 입니다." %(width, height, result))
